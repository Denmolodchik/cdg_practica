<p id="notice"><%= notice %></p>

<h1> Posts All </h1>

<tbody>
    <% @posts.each do |post| %>
        <tr>
        <td><%= post.title %></td></br>
        <td><%= image_tag post.image_url, size:250 %></td></br>
        <td><%= link_to 'Show', user_post_path(@user,post) %></td>
        </br></br>
        </tr>
    <%end%>
</tbody>